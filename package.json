{
  "name": "nodejs-auth-server",
  "version": "1.0.0",
  "description": "This is a boilerplate Node.js Express application showcasing basic usage of routes, database setup, and linting.",
  "main": "./dist/src/server.js",
  "types": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts && npm run format",
    "format": "prettier --write \"**/*.{json,md,ts,tsx}\"",
    "dev": "tsx watch -r tsconfig-paths/register ./src/server.ts",
    "build": "tsc",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/server.js",
    "prod": "npm run lint && npm run build && npm run start"
  },
  "author": "Tushar Popli",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.31.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^7.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "tsx": "^4.7.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.55.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  }
}
